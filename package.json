{
  "name": "coc-translator",
  "version": "1.5.3",
  "description": "Translation extension for coc.nvim",
  "main": "lib/index.js",
  "publisher": "voldikss",
  "keywords": [
    "coc.nvim",
    "translation",
    "translator"
  ],
  "engines": {
    "coc": "^0.0.79"
  },
  "homepage": "https://github.com/voldikss/coc-translator",
  "scripts": {
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "translator.toLang": {
          "type": "string",
          "default": "zh",
          "description": "Target language"
        },
        "translator.engines": {
          "type": "array",
          "default": [
            "bing",
            "google",
            "youdao",
            "haici"
          ]
        },
        "translator.enableHover": {
          "type": "boolean",
          "default": false,
          "description": "Enable on hover translating"
        },
        "translator.maxsize": {
          "type": "number",
          "default": 5000,
          "description": "Max history count"
        },
        "translator.window.maxWidth": {
          "type": "number",
          "default": 999
        },
        "translator.window.maxHeight": {
          "type": "number",
          "default": 999
        }
      }
    },
    "commands": [
      {
        "title": "show translation",
        "command": "translator.popup"
      },
      {
        "title": "Echo translation",
        "command": "translator.echo"
      },
      {
        "title": "replace word with translation",
        "command": "translator.replace"
      },
      {
        "title": "open history file",
        "command": "translator.exportHistory"
      }
    ]
  },
  "author": "dyzplus@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@types/uuid": "^8.3.0",
    "@types/xml2js": "^0.4.7",
    "@voldikss/tsconfig": "^1.0.0",
    "@voldikss/tslint-config": "^1.0.6",
    "coc.nvim": "^0.0.79",
    "request-light": "^0.4.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.11",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2",
    "uuid": "^8.3.1",
    "webpack": "^5.8.0",
    "webpack-cli": "^4.2.0",
    "xml2js": "^0.4.23"
  }
}
